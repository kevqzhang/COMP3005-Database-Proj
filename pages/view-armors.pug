html 
    head
        title Armor
        style 
            include ../public/styles/navbar.css
            include ../public/styles/armors.css
    body 
        include navbar
        .content
            .characters 
                h1 Characters: 
                each char in characters
                    .character 
                        a(href="/characters/" + char.cid) #{char.name + " - Level " + char.charLvl + " " + char.subclass + " " + char.class}
            .armor 
                h1 Armor: 
                each arm in armors
                    .armor 
                        a(href="/armor/" + arm.armorName) #{arm.armorName}